{% extends "base.jinja2" %}
{% block content %}
    <div class="col-md-10">
        <div class="content">
            <p tal:condition="logged_in" class="pull-right">
                <a href="${request.application_url}/logout">Logout</a>
            </p>
        </div>
    </div>
    <ul>List of Entries
        {% for entry in entry_list %}
        <li>
            <a href='{{request.route_url('entry', id=entry.id)}}'> {{ entry.title }}: {{ entry.created }}</a>
        </li>
        {% endfor %}
    </ul>
{% endblock content %}